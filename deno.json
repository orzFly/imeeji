    "@std/assert": "jsr:@std/assert@1",
    "@std/cli": "jsr:@std/cli@^1.0.27",
    "@std/semver": "jsr:@std/semver@^1.0.8",
    "@types/react": "npm:@types/react@^19.2.14",
    "react": "npm:react@^19.2.4",
    "react/jsx-runtime": "npm:react@^19/jsx-runtime",
    "ink": "npm:ink@^5.2.1"
  }
